{
    "name": "@asgardeo/auth-react",
    "version": "1.1.13",
    "description": "Asgardeo Auth React SDK for React Applications.",
    "keywords": [
        "WSO2",
        "WSO2-IS",
        "WSO2 Identity Server",
        "WSO2 Identity Apps",
        "authentication",
        "OIDC",
        "SDK",
        "Javascript",
        "Asgardeo",
        "React"
    ],
    "main": "dist/main.js",
    "types": "dist/src/index.d.ts",
    "module": "dist/main.js",
    "scripts": {
        "lint": "eslint --ext .js,.ts .",
        "fix-lint": "eslint --ext .js,.ts . --fix",
        "build": "rimraf dist && npm run type-check && webpack -p --env.NODE_ENV=production",
        "build:dev": "rimraf dist && npm run type-check && webpack -p --env.NODE_ENV=development",
        "type-check": "tsc --emitDeclarationOnly",
        "type-check:watch": "npm run type-check -- --watch"
    },
